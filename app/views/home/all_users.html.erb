<table class="table">
  
    <th> No </th>
    <th> Email </th>
  <% @users.each_with_index do |user, index| %>
    <tr>    
      <td><%=index+1 %></td>
      <td><%= user.email%></td>
       <td><%= image_tag user.image,:width => 50 %>
      <td><%#= link_to 'edit', edit_user_registration_url(user) %></td>
      <td><%#= link_to 'Destroy', user_registration_path(user.id), :confirm => 'Are you sure you want to destroy your account?', :method => :delete %></td>
      <td><%#= link_to 'show', show_user_path(user.id) %></td>
    </tr>
<% end %>
</table>


